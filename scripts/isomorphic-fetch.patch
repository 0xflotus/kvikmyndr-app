From 2c517fb009058949acbaa8bdfd14d0cda26d253f Mon Sep 17 00:00:00 2001
From: Birkir Gudjonsson <birkir.gudjonsson@gmail.com>
Date: Sat, 23 Sep 2017 14:21:02 -0700
Subject: [PATCH] React native support

---
 fetch-npm-react-native.js | 1 +
 package.json              | 1 +
 2 files changed, 2 insertions(+)
 create mode 100644 fetch-npm-react-native.js

diff --git a/fetch-npm-react-native.js b/fetch-npm-react-native.js
new file mode 100644
index 0000000..7e7ac17
--- /dev/null
+++ b/fetch-npm-react-native.js
@@ -0,0 +1 @@
+module.exports = fetch;
diff --git a/package.json b/package.json
index 85196b1..c232c4d 100644
--- a/package.json
+++ b/package.json
@@ -4,6 +4,7 @@
   "description": "Isomorphic WHATWG Fetch API, for Node & Browserify",
   "browser": "fetch-npm-browserify.js",
   "main": "fetch-npm-node.js",
+  "react-native": "fetch-npm-react-native.js",
   "scripts": {
     "files": "find . -name '*.js' ! -path './node_modules/*' ! -path './bower_components/*'",
     "test": "jshint `npm run -s files` && lintspaces -i js-comments -e .editorconfig `npm run -s files` && mocha"
